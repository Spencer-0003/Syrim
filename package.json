    "statcord-eris": "1.1.4"
  },
  "devDependencies": {
    "@types/node": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "eslint": "8.19.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-sonarjs": "0.13.0",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "prisma": "4.0.0",
